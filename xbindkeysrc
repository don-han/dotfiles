# Increase brightness
"xbacklight -inc 5"
    XF86MonBrightnessUp
    m:0x0 + c:233

# Decrease brightness
"xbacklight -dec 5"
    XF86MonBrightnessDown
    m:0x0 + c:232

# Toggle mute/unmute
"amixer sset Master toggle"
    XF86AudioMute
    m:0x0 + c:121

# Increase volume
"amixer sset Master 5%+ -M"
    XF86AudioRaiseVolume
    m:0x0 + c:123

# Decrease volume
"amixer sset Master 5%- -M"
    XF86AudioLowerVolume
    m:0x0 + c:122

# Open internet (Google Chrome)
"google-chrome-stable"
    Shift+Alt + i
    m:0x9 + c:31

"systemctl hibernate"
    XF86Launch1
    m:0x0 + c:156
